{
"extends": "@tsconfig/node16/tsconfig.json",
"compilerOptions": {
  "outDir": "dist",
  "target": "es2018",
    "module": "commonjs",
    "lib": ["es2018", "esnext.asynciterable"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false
  },
"include": ["src"],
"exclude": ["node_modules"]
}
